<Window x:Class="Assignments.View.Windows.ReturnWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignments.View.Windows"
        mc:Ignorable="d"
        Title="ReturnWindow" Style="{StaticResource returnwin}" WindowStartupLocation="CenterScreen">
    <Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="700"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<TextBlock x:Name="MsgTxt" Grid.Row="0" Style="{StaticResource returntext}"/>
		<Button Content="OK" Grid.Row="1" Click="CloseClick" Style="{StaticResource standardbutton}"/>
	</Grid>
</Window>
